{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${project.group}",
    "id": "${project.ext.id}",
    "version": "${project.version}",
    "metadata": {
      "name": "${project.ext.name}",
      "description": "${project.description}",
      "icon": {
        "64": "icon-x64.png",
        "128": "icon-x128.png",
        "256": "icon.png",
        "512": "icon-x512.png"
      }
    },
    "intermediate_mappings": "org.quiltmc:hashed"
  }
}